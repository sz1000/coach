page {
  height: 100%;
  background: #f5f5f5;
}

.order {
  height: 100%;
  background: #f5f5f5;
  margin: 88rpx 0;
}

.box {
  padding: 0 60rpx;
}

.head {
  text-align: center;
  margin-bottom: 40rpx;
}

.title {
  font-family: "FZSuXSLSJW3";
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
}

.logo {
  font-family: "FZSuXSLSJW11";
}

.reserve_time {
  font-family: "FZSuXSLSJW14";
}

.time {
  font-size: 30rpx;
  height: 64rpx;
  line-height: 64rpx;
}

.week {
  font-family: "FZSuXSLSJW4";
}

.line {
  width: 90%;
  height: 0rpx;
  border-bottom: 1rpx dashed #ccc;
  margin: auto;
}

.code {
  width: 100%;
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.codeBg {
  width: 330rpx;
  height: 330rpx;
}

.info {
  padding: 0 30rpx;
}

.caption {
  font-family: "FZSuXSLSJW4";
  font-size: 32rpx;
  height: 64rpx;
  line-height: 64rpx;
}

.address {
  font-size: 28rpx;
  line-height: 48rpx;
}

.icon {
  width: 32rpx;
  height: 32rpx;
  line-height: 48rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.tel {
  font-family: "FZSuXSLSJW14";
  font-size: 28rpx;
  height: 48rpx;
  line-height: 48rpx;
}

.all {
  font-family: "FZSuXSLSJW14";
}

.range {
  font-size: 28rpx;
  height: 48rpx;
  line-height: 48rpx;
}

.foot {
  display: flex;
  justify-content: space-around;
  text-align: center;
  font-size: 28rpx;
  padding: 20rpx 0;
  margin: 20rpx auto;
}

.edit {
  font-family: "FZSuXSLSJW4";
  width: 100rpx;
  border-bottom: 2rpx solid #333;
  font-size: 24rpx;
  text-align: center;
  margin: 0 auto;
}

.cancel {
  font-family: "FZSuXSLSJW4";
  width: 100rpx;
  border-bottom: 2rpx solid #333;
  font-size: 24rpx;
  text-align: center;
  margin: 0 auto;
}